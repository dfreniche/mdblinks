{
    "collection": "routes",
    "database": "routes",
    "roles": [
        {
            "name": "readAndWriteOwn",
            "apply_when": {
                "owner": "%%user.id"
            },
            "read": true,
            "write": true,
            "insert": true,
            "delete": true,
            "search": true
        },
        {
            "name": "readAndWriteAll",
            "apply_when": {},
            "read": true,
            "write": true,
            "insert": true,
            "delete": false,
            "search": true
        }
    ]
}
